<div class="block">
  <div class="secondary-navigation">
    <ul>
      <li class="first"><%= link_to 'List', admin_gigs_path  %></li>
      <li><%= link_to 'New', new_admin_gig_path %></li>
      <li class="active"><%= link_to 'Show', admin_gig_path %></li>
    </ul>
    <div class="clear"></div>
  </div>          
  <div class="content">
    <div class="inner">
      
        <p>
          <b>Date:</b>
          <%= @gig.date %>
        </p>

        <p>
          <b>Time:</b>
          <%= @gig.time %>
        </p>

        <p>
          <b>Venue:</b>
          <%= @gig.venue %>
        </p>

        <p>
          <b>Venue_url:</b>
          <%= @gig.venue_url %>
        </p>

        <p>
          <b>City:</b>
          <%= @gig.city %>
        </p>

        <p>
          <b>Country:</b>
          <%= @gig.country %>
        </p>

        <p>
          <b>Tickets_url:</b>
          <%= @gig.tickets_url %>
        </p>

        <p>
          <b>Permalink:</b>
          <%= @gig.permalink %>
        </p>
      <%= link_to 'Edit', edit_admin_gig_path(@gig) %>
    </div>
  </div>
</div>