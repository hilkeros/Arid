<div class="block">
  <div class="secondary-navigation">
    <ul>
      <li class="first"><%= link_to 'List', admin_photos_path  %></li>
      <li class="active"><%= link_to 'New', new_admin_photo_path %></li>
    </ul>
    <div class="clear"></div>
  </div>          
  <div class="content">            
    <h2 class="title">New Photo</h2>
    <div class="inner">
      <% form_for :photo, :url => admin_photos_path, :html => { :class => :form, :multipart => true } do |f| -%>
				<%= f.error_messages %>
				
        <%= render :partial => "form", :locals => {:f => f} %>
      <% end -%>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>