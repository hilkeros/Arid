<div class="block">
  <div class="secondary-navigation">
    <ul>
      <li class="first"><%= link_to 'List', admin_photos_path  %></li>
      <li><%= link_to 'New', new_admin_photo_path %></li>
      <li class="active"><%= link_to 'Show', admin_photo_path %></li>
    </ul>
    <div class="clear"></div>
  </div>          
  <div class="content">
    <div class="inner">
      
        <p>
          <b>Image_file_name:</b>
          <%= @photo.image_file_name %>
        </p>

        <p>
          <b>Image_content_type:</b>
          <%= @photo.image_content_type %>
        </p>

        <p>
          <b>Image_file_size:</b>
          <%= @photo.image_file_size %>
        </p>

        <p>
          <b>Image_updated_at:</b>
          <%= @photo.image_updated_at %>
        </p>

        <p>
          <b>User_id:</b>
          <%= @photo.user_id %>
        </p>

        <p>
          <b>Gig_id:</b>
          <%= @photo.gig_id %>
        </p>
      <%= link_to 'Edit', edit_admin_photo_path(@photo) %>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>